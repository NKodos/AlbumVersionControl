<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:AlbumVersionControl">


    <SolidColorBrush x:Key="DarkBaseBrush" Color="#3553BA" />
    <Color x:Key="DarkBaseColor">#3553BA</Color>

    <SolidColorBrush x:Key="MediumBaseBrush" Color="#4065E0" />
    <Color x:Key="MediumBaseColor">#4065E0</Color>

    <SolidColorBrush x:Key="LightBaseBrush" Color="#FF5F82F5" />
    <Color x:Key="LightBaseColor">#FF5F82F5</Color>

    <SolidColorBrush x:Key="UltraLightBaseBrush" Color="#FF88A4FF" />
    <Color x:Key="UltraLightBaseColor">#FF88A4FF</Color>


    <SolidColorBrush x:Key="DarkBrush" Color="#FFD6D6D6" />
    <Color x:Key="DarkColor">#FFD6D6D6</Color>

    <SolidColorBrush x:Key="MediumBrush" Color="#FFF1F1F1" />
    <Color x:Key="MediumColor">#FFF1F1F1</Color>

    <SolidColorBrush x:Key="LightBrush" Color="white" />
    <Color x:Key="LightColor">white</Color>

</ResourceDictionary>